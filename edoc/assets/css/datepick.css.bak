	}
	*/
.menu li ul{
	list-style:none;
    position:absolute;/**/
    display:none;width:130px;
}

.menu li:hover ul{		
  /*  top:35px;*/
    display:block;
	
}
/*sub2*/
.menu li ul li{
    color:#fff; background:url(../images/menu/three00.png); 
	position:relative;
	text-align:center; z-index:10000 !important;
}
/*
.menu li:hover ul li:hover{
    color:#fff; background:#000 url(../images/menu/three1.png);
}  repeat-x right top
*/
.menu li ul li a{
    color:#fff;
    cursor:pointer;
    width:130px;
	border:#FFF;
}

 .menu li ul li a:hover {
	color:#fff;  background:url(../images/menu/three2.png);
	}
	
.menu li ul li a:hover b {
	background:#fff url(../images/menu/three2.png)  repeat-x  right top;
	} 

	/*-----start 3rd level----*/
.menu li ul li ul{
	background:#fff url(../images/menu/3a.png);
}

.menu li:hover ul li ul{		
    position:absolute;
    display:none;	
}

.menu li:hover ul li:hover ul{	   
    display:inline-block;
	left:138px;
    top:0;
	
}

.menu li ul li ul li{
     background:#fff url(../images/menu/three01.png) ; 
	position:relative; 
	text-align:center; z-index:20000 !important;overflow:hidden;
}

.menu li ul li ul li a{
    color:#fff;
    cursor:pointer;   
	border:#FFF;
}

 .menu li ul li ul li a:hover {
	color:#fff;background:#fff url(../images/menu/three2.png)  repeat-x right top;
	}
	
.menu li ul li ul li a:hover b {
	background:#fff url(../images/menu/three1-2.png)  repeat-x  right top;
	} 

/**/
/*-----end 3rd level----*/

