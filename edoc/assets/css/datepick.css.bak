.menu {
	clear:both;
	padding:0 0 0 200px;/* */
	margin:0 0 0 0 ;
	width:100%;
	list-style:none; 
	height:34px; 
	background:#fff url(../images/menu/1a.png); 
	position:relative; 
	border:1px solid #690; 
	border-width:0 1px; 
	border-bottom:1px solid #690;		
	}
	
.menu li {
 	float:left; 
	}
	
.menu li a {	
	border-left:1px solid #690; 
	display:block;	
	height:34px;
	 line-height:34px; 
	color:#fff; 
	 text-decoration:none;
	 font-size:12px; font-family:arial, verdana, sans-serif;
	  font-weight:bold; 
	 text-align:center; 
	 padding:0 0 0 8px; 
	 cursor:pointer;	
	 }
	 
.menu li a b {
	float:left; display:block; padding:0 16px 0 8px;
	}
	
	.menu li.home a {
	color:#fff; background:url(../images/menu/home.png) no-repeat center top;
	}
	
.menu li.home a b {
	background:url(../images/menu/home.png) no-repeat center top;
	}

.menu li.home a:hover {
	color:#fff; background:#000 url(../images/menu/home2.png)  no-repeat center top; cursor:default;
	}
	
.menu li.home a:hover b {
	background:url(../images/menu/home2.png) no-repeat center top;
	}
	
.menu li.current a {
	color:#fff; background:url(../images/menu/three2.png);
	}
	
.menu li.current a b {
	background:url(../images/menu/three2.png) no-repeat right top;
	}
	
 .menu li a:hover {
	color:#fff; background:#000 url(../images/menu/three1.png);
	}
/*	
.menu li a:hover b {
	background:url(../images/menu/three1.png) no-repeat right top;
	} 
	
.menu li.current a:hover {
	color:#fff; background:#000 url(../images/menu/three2.png); cursor:default;
	}
	
.menu li.current a:hover b {
